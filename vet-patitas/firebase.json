{
  "database": {
      "rules": "database.rules.json"
      },
      "hosting": {
          "public": "dist",
          "ignore": [
              "**/node_modules/**",
              "**/src/**",
              "**/public/**"
          ],
          "rewrites": [{
              "source": "**",
              "destination": "/index.html"
          }
      ]
  }
}